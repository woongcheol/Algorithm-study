def solution(progresses, speeds):
    answer = []
    cnt = 0
    while progresses != []:
        for i in range(len(progresses)):
            if progresses[i] < 100:
               progresses[i] += speeds[i]
        while progresses != [] and progresses[0] >= 100:
            del progresses[0]
            cnt += 1
            if progresses == []:
                answer.append(cnt)
                break
            if progresses[0] < 100:
                answer.append(cnt)
                cnt = 0
                break
    return answer

progresses = [95, 90, 99, 99, 80, 99]	
speeds = [1, 5, 2, 1, 1, 1]
print(solution(progresses, speeds))